/dev/mapper/vg_work-lv_root /                       ext4  defaults  1 1
UUID=7c292bbc-1346-4e38-a771-1986e0c435ea /boot     ext4  defaults  1 2
/dev/mapper/vg_work-lv_home /home                   ext4  defaults  1 2
/dev/mapper/vg_work-lv_swap swap                    swap  defaults  0 0
